<!DOCTYPE HTML>
<html ng-app="AppClinic">
<head>
    <title>Clinic - Binus University Alam Sutera</title>
    <link rel="shortcut icon" href="~/Images/GreenCross.ico" type="image/x-icon"/>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    
    <!-- Custom Theme files -->
    <link href="~/Content/style.css" rel="stylesheet" />
    <link href="~/fonts/flaticon.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.css" rel="stylesheet" />
    <script src="~/scripts/jquery-3.1.0.min.js"></script>
    <script src="~/scripts/bootstrap.min.js"></script>
    <!-- Mainly scripts -->
    <script src="~/scripts/jquery.metisMenu.js"></script>
    <script src="~/scripts/jquery.slimscroll.min.js"></script>
    <!-- Custom and plugin javascript -->
    <script src="~/scripts/custom.js"></script>
    <link href="~/Content/custom.css" rel="stylesheet" />
    <script src="~/scripts/jquery.nicescroll.js"></script>
    <script src="~/scripts/scripts.js"></script>
    <!-- Angular and main module -->
    <script src="~/scripts/angular.min.js"></script>
    <script src="~/App/main.js"></script>
</head>
<body>
    <div id="wrapper">

        <!----->
        <nav class="navbar-default navbar-static-top" role="navigation">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <h1> <a class="navbar-brand" href="@Url.Action("Index","Home")">Clinic</a></h1>
                <h6> <a class="navbar-brand" href="https://binus.ac.id/" target="_blank"> @@ Binus University Alam Sutera</a></h6>
            </div>
                
                <div class="clearfix">
                    <!--banner-->
                    <div class="banner bread-crumb">

                        <h2>
                            <a href="@Url.Action("Index","Home")">Home</a>
                            <i class="fa fa-angle-right"></i>
                            <span>@ViewBag.Title</span>
                        </h2>
                    </div>
                    <!--//banner-->
                </div>

                <div class="navbar-default sidebar" role="navigation">
                    <div class="sidebar-nav navbar-collapse">
                        <ul class="nav" id="side-menu">

                            <li>                      
                                <a href="@Url.Action("Index","Home")" class="hvr-bounce-to-right">                
                                <i class="fa fa-dashboard nav_icon "></i><span class="nav-label">Dashboard</span> </a>
                            </li>

                            <li>
                                <a href="#" class="hvr-bounce-to-right"><i class="fa fa-users nav_icon"></i> <span class="nav-label">Visitors</span><span class="fa arrow"></span></a>
                                <ul class="nav nav-second-level">
                                    <li><a href="@Url.Action("DailyVisitors","Visitors")" class="hvr-bounce-to-right"><i class="fa flaticon-medical-records getleft nav_icon"></i>Daily Visitor List</a></li>
                                    <li><a href="@Url.Action("SectionList","Visitors")" class="hvr-bounce-to-right"> <i class="fa flaticon-first-aid-kit getleft nav_icon"></i>List Based on Section</a></li>
                                    <li><a href="@Url.Action("Hospitalized","Visitors")" class="hvr-bounce-to-right"><i class="fa flaticon-stethoscope getleft nav_icon"></i>Hospitalized Visitor</a></li>
                                </ul>
                            </li>
                            <li>
                                <a href="#" class="hvr-bounce-to-right"><i class="fa flaticon-ointment-1 getleft nav_icon"></i> <span class="nav-label">Medicines</span><span class="fa arrow"></span></a>
                                <ul class="nav nav-second-level">
                                    <li><a href="@Url.Action("Add","Medicine")" class="hvr-bounce-to-right"><i class="fa fa-heartbeat nav_icon"></i>Add Medicine</a></li>
                                    <li><a href="@Url.Action("Stock","Medicine")" class="hvr-bounce-to-right"><i class="fa fa-line-chart nav_icon"></i>Stock</a></li>
                                </ul>
                            </li>

                            <li>
                                <a href="@Url.Action("ExportExcel","Other")" class="hvr-bounce-to-right"><i class="fa fa-list nav_icon"></i> <span class="nav-label">Export to Excel </span></a>
                            </li>

                            <li>
                                <a href="#" class="hvr-bounce-to-right"><i class="fa fa-cog nav_icon"></i> <span class="nav-label">Settings</span><span class="fa arrow"></span></a>
                                <ul class="nav nav-second-level">
                                    <li><a href="@Url.Action("ChangePass","Other")" class="hvr-bounce-to-right"><i class="fa fa-refresh nav_icon"></i>Change Password</a></li>
                                </ul>
                            </li>
                            <li class="img">
                                <img src="~/Images/binus.png" class="brandImg"/>
                                <img src="~/Images/hec.png" class="brandImg"/>
                            </li>
                        </ul>
                    </div>
                </div>
        </nav>

        <div id="page-wrapper" class="gray-bg dashbard-1">
            <div class="content-main">

                <!--content-->
              
                    @RenderBody()

                <!----->
                   
                    <div class="clearfix"> </div>
                </div>
             </div>
                <!--//content-->
                <!---->
                <div class="copy">
                    <p> &copy; 2016 Minimal. All Rights Reserved | Design by <a href="http://w3layouts.com/" target="_blank">W3layouts</a> 
                    <br />Powered by Helena Natanael &amp; Billy Darmawan</p>
                </div>
            </div>
            <div class="clearfix"> </div>
        
   
    <!---->
    <!--scrolling js-->

    <!--//scrolling js-->
</body>
</html>

